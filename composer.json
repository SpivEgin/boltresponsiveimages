{
  "name": "cdowdy/boltresponsiveimages",
  "description": "A Bolt Extension To Create Responsive Images.",
  "type": "bolt-extension",
  "keywords": [
    "srcset",
    "responsive images",
    "picturefill"
  ],
  "require": {
    "bolt/bolt": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Cory Dowdy",
      "email": "cory@corydowdy.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Bolt\\Extension\\cdowdy\\boltresponsiveimages\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bolt\\Extension\\cdowdy\\boltresponsiveimages\\Tests\\": "tests",
      "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
    }
  },
  "extra": {
    "bolt-assets": "web",
    "bolt-class": "Bolt\\Extension\\cdowdy\\boltresponsiveimages\\BoltResponsiveImagesExtension",
    "bolt-icon": "resp-icon.png",
    "bolt-screenshots": [
      "screenshots/screenshot1.png",
      "screenshots/screenshot2.png"
    ]
  }
}
